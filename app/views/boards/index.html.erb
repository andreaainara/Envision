<div class="col-md-6">
  <div class="boards">
    <% @boards.each do |board| %>
      <p>
        <strong><%= link_to board.name, board_path(board.user, board) %></strong>
        <%= link_to "x", delete_board_path(board.user, board), method: :delete %> <br>
      </p>
    <% end %>
  </div>
</div>
